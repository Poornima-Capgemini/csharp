@model IEnumerable<EMS.DAL.EmployServiceReference.Employ>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create new", "Create")
</p>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Empno)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Name)

        </th>
        <th>
            @Html.DisplayNameFor(model => model.Department)

        </th>
        <th>
            @Html.DisplayNameFor(model => model.Designation)

        </th>
        <th>
            @Html.DisplayNameFor(model => model.Basic)

        </th>
        <th></th>
    </tr>


    @foreach (var item in Model)
    {
    <tr>
        <td>
            @Html.DisplayFor(model => item.Empno)
        </td>
        <td>
            @Html.DisplayFor(model => item.Name)

        </td>
        <td>
            @Html.DisplayFor(model => item.Department)

        </td>
        <td>
            @Html.DisplayFor(model => item.Designation)

        </td>
        <td>
            @Html.DisplayFor(model => item.Basic)

        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { Empno = item.Empno})|
            @Html.ActionLink("Details", "Details", new { Empno = item.Empno })|
            @Html.ActionLink("Delete", "Delete", new { Empno = item.Empno })
        </td>
    </tr>
    }
</table>